<?php
namespace UserBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="Users")
 */

class User
{

    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
protected $id;
    
    /**
     * @ORM\Column(type="string", length=30)
     */
protected $email;
    
     /**
     * @ORM\Column(type="string", length=30)
     */
protected $username;

     /**
     * @ORM\Column(type="string", length=30)
     */
protected $password;
    
    
    
}
